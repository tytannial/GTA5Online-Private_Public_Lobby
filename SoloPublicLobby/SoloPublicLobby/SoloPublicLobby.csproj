<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon.ico" />
    <None Remove="ImageResources\5mods.jpg" />
    <None Remove="ImageResources\add.png" />
    <None Remove="ImageResources\gears.png" />
    <None Remove="ImageResources\Github.png" />
    <None Remove="ImageResources\help.png" />
    <None Remove="ImageResources\locked.png" />
    <None Remove="ImageResources\mpgh.PNG" />
